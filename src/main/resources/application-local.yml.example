# ================================
# 本地开发配置模板
# ================================
# 使用方法：
# 1. 复制此文件为 application-local.yml
# 2. 填入你的真实配置
# 3. application-local.yml 已在 .gitignore 中，不会被提交
#
# 启动时使用：
#   mvn spring-boot:run -Dspring.profiles.active=dev,local
# 或在 IDE 中设置 Active Profiles: dev,local

spring:
  datasource:
    password: 你的数据库密码

ai:
  openai:
    api-key: sk-你的OpenAI密钥

knowledge:
  pgvector:
    password: 你的PGVector密码

# 如果需要翻译功能
translation:
  enabled: true
  aws:
    access-key: 你的AWS_AccessKey
    secret-key: 你的AWS_SecretKey

